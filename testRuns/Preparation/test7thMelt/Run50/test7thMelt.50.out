#-----------------------------
#--- Model parameters
#-----------------------------

variable natom	equal 4000  #--- system size
variable Ntype	equal 5		#--- number of atom types
variable a      equal ${cutoff}   #--- lattice constant
variable a      equal 2.502362204724409   
variable Dt		equal 0.001 #--- discretization time
variable Tinit 	equal 300.0  #--- initial temperature
variable Tfinal equal 3000.0 #--- final temperature
variable tDampTemp 	equal 10*${Dt}   #--- thermostat equilibration time scale
variable tDampTemp 	equal 10*0.001   
variable Trate 	equal 50.0   #--- melting rate
variable Pinit	equal 1.0132  #--- number of time steps
variable Nfreq	equal 100  #--- number of time steps
variable tEquilibrate	equal 1000*${tDampTemp}  #--- equilibration time
variable tEquilibrate	equal 1000*0.01  
variable tDampPress 	equal ${tDampTemp}   #--- barostat equilibration time scale
variable tDampPress 	equal 0.01   

#-----------------------------
#--- Compute Variables
#-----------------------------
variable	nRunEquilibrate	equal	${tEquilibrate}/${Dt}
variable	nRunEquilibrate	equal	10/${Dt}
variable	nRunEquilibrate	equal	10/0.001
variable	tMelting		equal	(${Tfinal}-${Tinit})/${Trate}
variable	tMelting		equal	(3000-${Tinit})/${Trate}
variable	tMelting		equal	(3000-300)/${Trate}
variable	tMelting		equal	(3000-300)/50
variable	nRunMelting		equal	${tMelting}/${Dt}
variable	nRunMelting		equal	54/${Dt}
variable	nRunMelting		equal	54/0.001
#variable a		equal	${1/RHO^(1.0/3.0)} #--- lattice constant
variable lx		equal	${natom}^(1.0/3.0)*${a} #--- length
variable lx		equal	4000^(1.0/3.0)*${a} 
variable lx		equal	4000^(1.0/3.0)*2.50236220472441 

#-----------------------------
#--- system details
#-----------------------------
units		metal
atom_style	atomic
boundary	p p p

#-----------------------------
#--- read data file
#-----------------------------
#read_data       FeNi_glass_${initTemp}.data

#--- define box & lattice
region mybox block 0.0 ${lx} 0.0 ${lx} 0.0 ${lx}
region mybox block 0.0 39.7225239618499 0.0 ${lx} 0.0 ${lx}
region mybox block 0.0 39.7225239618499 0.0 39.7225239618499 0.0 ${lx}
region mybox block 0.0 39.7225239618499 0.0 39.7225239618499 0.0 39.7225239618499
create_box ${Ntype} mybox
create_box 5 mybox
